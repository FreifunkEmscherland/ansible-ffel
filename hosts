# Allgemeine Infrastruktur-Server 
[common]
buildserver ansible_ssh_host= ansible_ssh_port=
monitoring  ansible_ssh_host=

# Service-VM für Zentrale Dienste (Karte, Statistik)  
[services]
services    ansible_ssh_host=

# Backbone Server 
[backbone]

[gateways]
# Gateways mit oder ohne Direktanbindung FFRL
# Immer mit Multidomänen-L2TP
gw01.freifunk-emscherland.de	ansible_ssh_host=185.46.137.150
gw02.freifunk-emscherland.de	ansible_ssh_host=5.9.16.220

# Sonstige Server
[firmware]
firmware	ansible_ssh_host= ansible_ssh_port=

